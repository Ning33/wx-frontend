<!--pages/service/yldyhd/yldyhd-sbxx.wxml-->
<view class="page">
  <view class="page__bd">
    <hnisi-form model="{{sbxx}}" rules="{{rules}}" bindsubmit="handleNext">
      <view slot="content">
        <hnisi-list title="个人信息">
          <hnisi-input id="AAC003" label="姓名" value="{{sbxx.AAC003}}"/>
          <hnisi-input id="AAC002" label="证件号码" value="{{sbxx.AAC002}}"/>
          <hnisi-input id="AAE010" label="社保卡账号" value="{{sbxx.AAE010}}"/>
        </hnisi-list>

        <hnisi-list title="基本资料">
          <hnisi-input id="CAC031" label="户口所属社区居(村)委会" value="{{sbxx.CAC031}}" placeholder="请输入" bindinput="handleInput"/>
          <hnisi-input id="CAC542" label="籍贯" value="{{sbxx.CAC542}}" placeholder="请输入" bindinput="handleInput"/>
          <hnisi-input id="CIC520" label="是否异地定居" value="{{sbxx.CIC520}}" placeholder="请输入" bindinput="handleInput"/>
        </hnisi-list>

        <hnisi-list title="联系方式">
          <hnisi-input id="CAC554" label="手机号码" value="{{sbxx.CAC554}}" placeholder="请输入" bindinput="handleInput"/>
          <hnisi-input id="AAE005" label="固定电话" value="{{sbxx.AAE005}}" placeholder="请输入" bindinput="handleInput"/>
          <hnisi-input id="CAC552" label="现居住地址" value="{{sbxx.CAC552}}" placeholder="请输入" bindinput="handleInput"/>
          <hnisi-input id="CAC551" label="邮政编码" value="{{sbxx.CAC551}}" placeholder="请输入" bindinput="handleInput"/>        
        </hnisi-list>
      </view>
    </hnisi-form>
  </view>
</view>